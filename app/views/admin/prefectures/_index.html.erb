<table>
  <thead>
    <tr>
      <th></th>
      <th>都道府県名</th>
      <th>緯度</th>
      <th>経度</th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% prefectures.each do |prefecture| %>
      <tr>
        <td><%= image_tag prefecture.get_prefecture_image, size:'100x100' %></td>
        <td><%= prefecture.name %></td>
        <td><%= prefecture.latitude %></td>
        <td><%= prefecture.longitude %></td>
        <td><%= link_to 'Edit', edit_admin_prefecture_path(prefecture), class: "btn btn-success" %></td>
        <td><%= link_to 'Destroy', admin_prefecture_path(prefecture), method: :delete, data: { confirm: '本当に削除しますか？' }, class: "btn btn-danger" %></td>
      </tr>
    <% end %>
  </tbody>
</table>