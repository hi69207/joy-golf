  <h1>都道府県</h1>

    <div class='row'>
      <h2>登録フォーム</h2>

      <%= render 'layouts/errors', obj: @prefecture %>

      <%= form_with model: @prefecture, url: admin_prefectures_path, method: :post, local: true do |f| %>
        <div class="form-group">
          <%= f.label :name, "都道府県名" %>
          <%= f.text_field :name %>
        </div>
        <div class="form-group">
          <%= f.label :prefecture_image, "画像" %>
          <%= f.file_field :prefecture_image, accept: 'image/*' %>
        </div>
        <div class="form-group">
          <%= f.label :latitude, "緯度" %>
          <%= f.text_field :latitude %>
        </div>
        <div class="form-group">
          <%= f.label :longitude, "経度" %>
          <%= f.text_field :longitude %>
        </div>
        <div class="form-group">
          <%= f.submit '登録', class: 'btn btn-success' %>
        </div>
      <% end %>
    </div>

    <div class='row'>
      <h2>登録一覧</h2>
      <%= render 'index', prefectures: @prefectures %>
    </div>