<%= form_with model: prefecture, url: admin_prefectures_path, method: :post, local: true do |f| %>
  <div class="form-group">
    <%= f.label :name, "都道府県名" %>
    <%= f.text_field :name %>
  </div>
  <div class="form-group">
    <%= f.label :image, "画像" %>
    <%= f.file_field :prefecture_image, accept: 'image/*' %>
  </div>
  <div class="form-group">
    <%= f.label :name, "緯度" %>
    <%= f.text_area :latitude %>
  </div>
  <div class="form-group">
    <%= f.label :name, "経度" %>
    <%= f.text_area :longitude %>
  </div>
  <div class="form-group">
    <%= f.submit '登録', class: 'btn btn-success' %>
  </div>
<% end %>
